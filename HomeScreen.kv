<HomePage>:
    name: 'home'
    MDToolbar:
        id: toolbar
        pos_hint: {"top": 1}
        elevation: 10
        title: "NOME DO APP - Home"
        md_bg_color: toolbarColor


    MDBottomNavigation:
        size_hint_y: .09
        panel_color: get_color_from_hex("#F6C7B7")
        text_color_active: get_color_from_hex("#B42529")
        
        MDBottomNavigationItem:
            name: 'screen 1'
            text: 'Home'
            icon: 'home-circle'
            on_enter: root.switchHome()

        MDBottomNavigationItem:
            name: 'screen 2'
            text: 'Menstrual'
            icon: 'atom-variant'
            on_enter:
                root.manager.transition.direction = 'left'
                root.switchHome()

        MDBottomNavigationItem:
            name: 'screen 3'
            text: 'Reprodutivo'
            icon: 'cards-heart'
            on_enter:
                root.manager.transition.direction = 'left'
                root.switchHome()
        
        MDBottomNavigationItem:
            name: 'screen 4'
            text: 'Botao 4'
            icon: 'cash'
            on_enter: root.switchHome()
